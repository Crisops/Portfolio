---
import ContactShoting from "../react/ContactShoting";
import ContactStar from "../react/ContactStar";
import ContactPlanet from './ContactPlanet.astro'
import ContactEarth from "./ContactEarth.astro";
import Contact from '../react/Contact'
import ContactFormBorder from './ContactFormBorder.astro'
import SocialNetworks from "../react/SocialNetworks";
---

<section id="contact" class="relative h-screen overflow-hidden bg-white dark:bg-[#0F0101]">
  <Contact client:visible>
    <ContactFormBorder/>
  </Contact>
  <ContactPlanet>
    <ContactStar client:visible/>
    <ContactShoting client:visible/>
    <ContactEarth/>
  </ContactPlanet>
  <SocialNetworks client:visible/>
</section>